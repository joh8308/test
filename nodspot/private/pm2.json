{
  "apps": [{
    "name": "spot27",
    "log_date_format": "YYYY-MM-DD HH:mm:ss SSS",
    "error_file": "/var/log/spot27/err.log",
    "out_file": "/var/log/spot27/app.log",
    "max_memory_restart": "1024M",
    "script": "/opt/nod-bizware/spot27/main.js",
    "env": {
      "METEOR_SETTINGS": {
        "mode": "production",
        "private": {
          "mode": "production-private",
          "files": {
            "upload": "/opt/nod-bizware/spot27_files/upload",
            "chat": {
              "origin": "/opt/nod-bizware/spot27_files/chat/origin",
              "thumb": "/opt/nod-bizware/spot27_files/chat/thumb"
            },
            "profile": {
              "origin": "/opt/nod-bizware/spot27_files/profile/origin",
              "thumb": "/opt/nod-bizware/spot27_files/profile/thumb"
            }
          },
          "push": {
            "apple": {
              "productionMode": "true",
              "gatewayHost": "",
              "crtFilePath": "",
              "keyFilePath": ""
            },
            "google": {
              "api": "",
              "sender": ""
            }
          },
          "restapi": "/m/spot27/restapi/",
          "sms": {
            "url": "",
            "user": "",
            "password": "",
            "senderNumber": ""
          }
        },
        "public": {
          "mode": "production-public"
        }
      },
      "MONGO_OPLOG_URL": "mongodb://localhost:27017/local",
      "MONGO_URL": "mongodb://localhost:27017/spot27",
      "NODE_ENV": "production",
      "PORT": "3060",
      "ROOT_URL": "http://127.0.0.1:3060/m/spot27/"
    }
  }]
}
